{
    "name": "simple-boilerplate",
    "version": "1.0.0",
    "private": true,
    "description": "Simple Boilerplate",
    "keywords": [
        "gatsby"
    ],
    "scripts": {
        "dev:gatsby": "gatsby develop -H 0.0.0.0",
        "build": "SET NODE_ENV=production yarn build:css && gatsby build",
        "build:github": "yarn build:css && gatsby build",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "build:css": "tailwindcss -i ./src/styles/tailwind/base.css -o ./src/styles/tailwind/build.css",
        "watch:css": "tailwindcss -i ./src/styles/tailwind/base.css -o ./src/styles/tailwind/build.css --watch",
        "dev": "NODE_ENV=development concurrently \"yarn dev:gatsby\" \"yarn watch:css\"",
        "dev:windows": "SET NODE_ENV=development concurrently \"yarn dev:gatsby\" \"yarn watch:css\""
    },
    "dependencies": {
        "@emotion/react": "^11.11.0",
        "@emotion/styled": "^11.11.0",
        "@fortawesome/fontawesome-free": "^6.4.0",
        "@fortawesome/fontawesome-svg-core": "^6.4.0",
        "@fortawesome/free-brands-svg-icons": "^6.4.0",
        "@fortawesome/free-solid-svg-icons": "^6.4.0",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "@splidejs/splide-extension-auto-scroll": "^0.5.3",
        "aos": "^3.0.0-beta.6",
        "autoprefixer": "^10.4.8",
        "browser-monads": "^1.0.0",
        "classnames": "^2.3.1",
        "fs-extra": "^11.2.0",
        "gatsby": "^5.14.0",
        "gatsby-plugin-alias-imports": "^1.0.5",
        "gatsby-plugin-image": "^3.14.0",
        "gatsby-plugin-manifest": "^5.14.0",
        "gatsby-plugin-preact": "^7.14.0",
        "gatsby-plugin-react-helmet": "^6.14.0",
        "gatsby-plugin-react-helmet-canonical-urls": "^1.4.0",
        "gatsby-plugin-react-i18next": "^3.0.1",
        "gatsby-plugin-remove-trailing-slashes": "^2.10.0",
        "gatsby-plugin-robots-txt": "^1.7.1",
        "gatsby-plugin-sharp": "^5.14.0",
        "gatsby-plugin-sitemap": "^6.14.0",
        "gatsby-source-filesystem": "^5.14.0",
        "gatsby-transformer-json": "^5.14.0",
        "gatsby-transformer-sharp": "^5.14.0",
        "i18next": "^21.10.0",
        "i18next-browser-languagedetector": "^7.2.1",
        "i18next-xhr-backend": "^3.2.2",
        "postcss": "^8.4.16",
        "preact": "^10.10.6",
        "preact-render-to-string": "^5.2.2",
        "react": "17.0.2",
        "react-device-detect": "^2.2.3",
        "react-dom": "17.0.2",
        "react-helmet": "^6.1.0",
        "react-i18next": "^11.18.6",
        "react-icons": "^4.4.0",
        "react-intersection-observer": "^9.4.0",
        "react-multi-carousel": "^2.8.2",
        "react-responsive": "^9.0.2",
        "react-smooth-collapse": "^2.1.2",
        "tailwindcss": "^3.1.8"
    },
    "devDependencies": {
        "concurrently": "^7.3.0",
        "dotenv": "^16.0.1"
    }
}
